[project]
	description = Docker - the open-source application container engine
[access]
	inheritFrom = All-Projects
[access "refs/*"]
	owner = group user/baroque1990
[access "refs/for/refs/*"]
	read = group user/baroque1990
	push = group user/baroque1990
	label-Code-Review = -2..+2 group user/baroque1990
	label-Verified = -1..+1 group user/baroque1990
	rebase = group user/baroque1990
	removeReviewer = group user/baroque1990
	submit = group user/baroque1990
[access "refs/heads/*"]
	read = group user/baroque1990
	create = group user/baroque1990
	push = +force group user/baroque1990
	pushMerge = group user/baroque1990
[access "refs/tags/*"]
	push = group user/baroque1990
	push = +force group user/baroque1990
